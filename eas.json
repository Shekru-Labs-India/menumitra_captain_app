{
  "cli": {
    "version": ">= 14.0.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk",
        "package": "com.menumitra.captainapp.preview"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk",
        "package": "com.menumitra.captainapp.preview"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "buildType": "apk",
        "credentialsSource": "local",
        "package": "com.menumitra.captainapp"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
